<h1>Add a Book</h1>
<%= form_for @book do |f| %>
	<p>
		<%= f.label :title%> <br />
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :author%> <br />
		<%= f.text_field :author %>
	</p>
	<p>
		<%= f.label :publisher%> <br />
		<%= f.text_field :publisher %>
	</p>
	<p>
		<%= f.label :edition%> <br />
		<%= f.number_field :edition %>
	</p>
	<p>
		<%= f.label :isbn%> <br />
		<%= f.text_field :isbn %>
	</p>
	<p>
		<%= f.button "Submit Book" %> 
	</p>
<% end %>